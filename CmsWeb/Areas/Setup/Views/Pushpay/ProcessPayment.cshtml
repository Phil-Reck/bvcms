@using CmsWeb.Controllers
@{
    Layout = "~/Views/Shared/OnlineReg2.cshtml";
    ViewBag.Title = "Pushpay Payment";
}
@section scripts
{
    @Fingerprint.Script("/Content/touchpoint/js/onlinereg/OnlineReg2.js")
}

<div class="confirm">
    <h2>Transaction Completed</h2>
    @ViewBag.Message
    <br />
    <a href="/OnlineReg/@ViewBag.OrgId">Start a new transaction</a>
    @if (MobileAppMenuController.InMobileAppMode)
    {
        <a href="@MobileAppMenuController.MobileAppReturnUrl" class="btn btn-primary">Return to Application</a>
    }
</div>
